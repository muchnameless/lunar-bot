		"@sapphire/shapeshift": "3.6.1-next.bf90479.0",
		"@sapphire/stopwatch": "1.4.2-next.945ecc2.0",
		"@sapphire/type": "2.2.5-next.cf34f61.0",
		"@sapphire/utilities": "3.9.4-next.945ecc2.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#1bd7ca09f831df70b43a1c4e1fccf1df10c00c7b",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.9.1",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.1.0",
		"discord.js": "14.3.1-dev.1663546265-6d43e26.0",
		"fast-xml-parser": "^4.0.10",
		"ioredis": "^5.2.3",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
		"lilyweight": "^2.7.0",
		"minecraft-data": "^3.12.0",
		"minecraft-protocol": "1.36.1",
		"ms": "3.0.0-canary.1",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"pino": "^8.5.0",
		"postgres": "3.2.4",
		"prettier": "^2.7.1",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "^6.23.0",
		"timezonecomplete": "^5.12.4",
		"tslib": "^2.4.0",
		"undici": "^5.10.0",
		"utf-8-validate": "^5.0.9",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-angular": "^17.1.0",
