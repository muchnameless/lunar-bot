		"pino": "^8.4.0",
		"postgres": "3.2.4",
		"prettier": "^2.7.1",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "^6.21.3",
