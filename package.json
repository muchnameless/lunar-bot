		"discord-api-types": "0.37.6-next.815c68f.1662307701",
		"discord.js": "patch:discord.js@14.4.0-dev.1662293040-8b3d006.0#./.yarn/patches/discord.js-npm-14.3.1-dev.1661774646-682e0e1.0-c4cc9c232f.patch",
		"minecraft-protocol": "patch:minecraft-protocol@npm:1.36.1#.yarn/patches/minecraft-protocol-npm-1.36.0-42ba3c97ef.patch"
	}
