{
	"name": "lunar-bot",
	"version": "2.0.0",
	"type": "module",
	"description": "dev version of the lunar guard discord bot",
	"main": "dist/index.js",
	"imports": {
		"#api": "./dist/api/index.js",
		"#chatBridge/*": "./dist/lib/chatBridge/*.js",
		"#chatBridge/constants": "./dist/lib/chatBridge/constants/index.js",
		"#constants": "./dist/lib/constants/index.js",
		"#db": "./dist/lib/structures/database/index.js",
		"#functions": "./dist/lib/functions/index.js",
		"#logger": "./dist/logger.js",
		"#networth/*": "./dist/lib/networth/*.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types": "./dist/lib/types/util.js",
		"#utils": "./dist/lib/util/index.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "yarn clean && tsc -b src",
		"update": "yarn upgrade-interactive",
		"test": "tsc -b src",
		"lint": "eslint src --ext ts --cache",
		"format": "prettier --cache --write . && eslint src --ext ts --fix --cache",
		"prepare": "husky install",
		"start": "scripts/save_commit.sh && env-cmd node . | tee -a logs/lunar-bot-out.log",
		"start:dev": "NODE_ENV=development yarn start | pino-pretty",
		"dev": "yarn build; yarn start:dev",
		"deploy:commands": "env-cmd node dist/scripts/deploy.js | pino-pretty",
		"db": "env-cmd node dist/scripts/dbInit.js --alter | pino-pretty",
		"migrate": "env-cmd ley"
	},
	"author": "muchnameless",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/builders": "0.16.0-dev.1657411897-f0b68d5",
		"@discordjs/collection": "0.8.0-dev.1657411895-f0b68d5",
		"@discordjs/rest": "0.6.0-dev.1657411905-f0b68d5",
		"@sapphire/async-queue": "1.3.2-next.688ed4a.0",
		"@sapphire/discord-utilities": "2.11.5-next.688ed4a.0",
		"@sapphire/shapeshift": "3.5.1-next.31b762e.0",
		"@sapphire/stopwatch": "1.4.2-next.688ed4a.0",
		"@sapphire/time-utilities": "1.7.5-next.688ed4a.0",
		"@sapphire/type": "2.2.5-next.cf34f61.0",
		"@sapphire/utilities": "3.7.1-next.688ed4a.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#b8e9632211c09bb6e97c7a9a96fe34109fcb2055",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.8.0",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.1.0",
		"discord.js": "14.0.0-dev.1657411900-f0b68d5",
		"fast-xml-parser": "^4.0.9",
		"ioredis": "^5.2.1",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
		"lex": "^1.7.9",
		"lilyweight": "^2.6.3",
		"minecraft-data": "^3.5.1",
		"minecraft-protocol": "^1.35.0",
		"ms": "^2.1.3",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"pino": "^8.1.0",
		"postgres": "3.2.4",
		"prettier": "^2.7.1",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "^6.21.3",
		"timezonecomplete": "^5.12.4",
		"tslib": "^2.4.0",
		"undici": "^5.8.0",
		"utf-8-validate": "^5.0.9",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-angular": "^17.0.3",
