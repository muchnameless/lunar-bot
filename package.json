		"ley": "^0.7.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^9.1.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.9.0-dev.20220904"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.3",
	"resolutions": {
		"@discordjs/builders": "patch:@discordjs/builders@1.3.0-dev.1662293019-8b3d006.0#./.yarn/patches/@discordjs-builders-npm-1.2.1-dev.1662163785-f79ccb4.0-1f5e0daf45.patch",
