		"minecraft-protocol": "1.36.1",
		"ms": "3.0.0-canary.1",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"pino": "^8.4.2",
		"postgres": "3.2.4",
		"prettier": "^2.7.1",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "^6.21.4",
		"timezonecomplete": "^5.12.4",
		"tslib": "^2.4.0",
		"undici": "^5.10.0",
		"utf-8-validate": "^5.0.9",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.1",
		"@commitlint/config-angular": "^17.1.0",
		"@types/common-tags": "^1.8.1",
		"@types/cron": "^2.0.0",
		"@types/node": "^18.7.13",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.7.0",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.35.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-regexp": "^1.9.0",
		"eslint-plugin-unicorn": "^43.0.2",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^9.1.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.9.0-dev.20220828"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.3",
	"resolutions": {
		"@discordjs/builders": "patch:@discordjs/builders@npm:1.2.1-dev.1661688251-682e0e1.0#.yarn/patches/@discordjs-builders-npm-1.1.1-dev.1660305925-c97977a.0-ba2033ace3.patch",
