{
  "name": "lunar-bot",
  "version": "2.0.0",
  "type": "module",
  "description": "dev version of the lunar guard discord bot",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "npm run clean && tsc --watch",
    "update": "git pull && npm run build && npm run start",
    "test": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/*",
    "prepare": "husky install",
    "start": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development npm start | pino-pretty",
    "dev": "npm run build && npm run start:dev",
    "deploy:commands": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/deploy.js | pino-pretty",
    "db": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/dbInit.js --alter | pino-pretty"
  },
  "author": "muchnameless",
  "license": "GPL-3.0",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@keyv/redis": "^2.2.2",
    "@sapphire/async-queue": "^1.1.9",
    "@sapphire/stopwatch": "^1.2.5",
    "@sapphire/time-utilities": "^1.5.2",
    "@sapphire/type": "^2.1.2",
    "@sapphire/utilities": "^3.2.1",
    "@skyra/jaro-winkler": "^1.0.0",
    "@zikeji/hypixel": "github:muchnameless/node-hypixel",
    "bufferutil": "^4.0.6",
    "chartjs-node-canvas": "^4.1.6",
    "common-tags": "^1.8.2",
    "cron": "^1.8.2",
    "discord.js": "14.0.0-dev.1643041968.2f16f87",
    "dotenv": "^14.2.0",
    "emoji-regex": "9.2.2",
    "formdata-polyfill": "^4.0.10",
    "js-big-decimal": "^1.3.4",
    "keyv": "^4.0.5",
    "lex": "^1.7.9",
    "lilyweight": "^2.1.0",
    "minecraft-data": "^2.110.0",
    "minecraft-protocol": "^1.30.0",
    "ms": "^2.1.3",
    "node-fetch": "^3.2.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino": "^7.6.4",
    "prismarine-chat": "^1.5.0",
    "prismarine-nbt": "^2.1.0",
    "re2": "^1.17.3",
    "readdirp": "^3.6.0",
    "rss-parser": "^3.12.0",
    "sequelize": "^6.14.0",
    "timezonecomplete": "^5.12.4",
    "tiny-typed-emitter": "^2.1.0",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.8"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cron": "^1.7.3",
    "@types/keyv": "^3.1.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.10",
    "@types/pg": "^8.6.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.26.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "pino-pretty": "^7.5.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.0-dev.20220124"
  },
  "engines": {
    "node": ">=v17.0.1",
    "npm": ">=8.3.2"
  }
}
