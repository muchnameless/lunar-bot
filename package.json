{
	"name": "lunar-bot",
	"version": "2.0.0",
	"type": "module",
	"description": "dev version of the lunar guard discord bot",
	"main": "dist/index.js",
	"scripts": {
		"clean": "node scripts/clean.mjs",
		"build": "yarn clean && tsc",
		"update": "yarn upgrade-interactive",
		"test": "tsc --noEmit",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write src/*",
		"prepare": "husky install",
		"start": "env-cmd node dist/index.js | tee -a logs/lunar-bot-out.log",
		"start:dev": "NODE_ENV=development yarn start | pino-pretty",
		"dev": "yarn build; yarn start:dev",
		"deploy:commands": "env-cmd node dist/scripts/deploy.js | pino-pretty",
		"db": "env-cmd node dist/scripts/dbInit.js --alter | pino-pretty",
		"migrate": "env-cmd ley"
	},
	"author": "muchnameless",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/builders": "*",
		"@discordjs/collection": "*",
		"@discordjs/rest": "*",
		"@sapphire/async-queue": "^1.3.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#6da21ece522ea8dd61f44b5c74ab64b2f0e8be82",
		"bree": "^8.0.2",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.7.1",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^1.8.2",
		"discord-api-types": "*",
		"discord.js": "14.0.0-dev.1649160496-402514f",
		"emoji-regex": "9.2.2",
		"fast-xml-parser": "^4.0.7",
		"ioredis": "^5.0.3",
		"js-big-decimal": "^1.3.4",
		"lex": "^1.7.9",
		"lilyweight": "^2.5.0",
		"minecraft-data": "^2.220.0",
		"minecraft-protocol": "^1.32.1",
		"ms": "^2.1.3",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"pino": "^7.9.2",
		"postgres": "3.0.4",
		"prismarine-chat": "^1.6.0",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.4",
		"sequelize": "6.18.0",
		"timezonecomplete": "^5.12.4",
		"tiny-typed-emitter": "^2.1.0",
		"tslib": "^2.3.1",
		"undici": "^5.0.0",
		"utf-8-validate": "^5.0.9"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-angular": "^16.2.3",
		"@types/common-tags": "^1.8.1",
		"@types/cron": "^1.7.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.23",
		"@types/pg": "^8.6.5",
		"@types/safe-timers": "^1.1.0",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-regexp": "^1.6.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"husky": "^7.0.4",
		"ley": "^0.7.1",
		"lint-staged": "^12.3.7",
		"pino-pretty": "^7.6.0",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.7.0-dev.20220405"
	},
	"engines": {
		"node": ">=v17.5.0",
		"npm": ">=8.4.1"
	},
	"packageManager": "yarn@3.2.0",
	"resolutions": {
		"@discordjs/builders": "0.13.0-dev.1649117272-905a6a1",
		"@discordjs/collection": "0.6.0-dev.1649160495-402514f",
		"@discordjs/rest": "0.4.0-dev.1649160494-402514f",
		"discord-api-types": "0.30.0"
	}
}
