		"env-cmd": "^10.1.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-regexp": "^1.8.0",
		"eslint-plugin-unicorn": "^43.0.2",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^9.0.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.9.0-dev.20220812"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
