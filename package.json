		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#5d0af5aedf8d9f6748d9e03a183a8dde3bc74338",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.8.0",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.0.0",
		"discord.js": "14.0.0-dev.1656060492-e24970e",
		"fast-xml-parser": "^4.0.8",
		"ioredis": "^5.1.0",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
		"lex": "^1.7.9",
