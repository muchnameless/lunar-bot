		"eslint-plugin-unicorn": "^43.0.1",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^8.1.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.8.0-dev.20220709"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.1",
	"resolutions": {
