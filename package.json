		"@sapphire/stopwatch": "1.4.2-next.1562630.0",
		"@sapphire/type": "2.2.5-next.cf34f61.0",
		"@sapphire/utilities": "3.9.3-next.1562630.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#fetch",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.9.1",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.1.0",
		"discord.js": "14.3.1-dev.1661688276-682e0e1.0",
		"fast-xml-parser": "^4.0.9",
		"ioredis": "^5.2.3",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
		"lilyweight": "^2.7.0",
