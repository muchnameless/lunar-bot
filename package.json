{
  "name": "lunar-bot",
  "version": "2.0.0",
  "type": "module",
  "description": "dev version of the lunar guard discord bot",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "npm run clean && tsc --watch",
    "update": "git pull && npm run build && npm run start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --no-stack-trace --verbose --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "start": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
    "start:dev": "npm start | pino-pretty",
    "dev": "npm run build && npm run start:dev",
    "deploy:commands": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/deploy.js",
    "maintenance": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/maintenance.js",
    "db": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/dbInit.js --alter | pino-pretty"
  },
  "author": "muchnameless",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "0.7",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@keyv/redis": "^2.1.3",
    "@sapphire/async-queue": "^1.1.8",
    "@sapphire/stopwatch": "^1.2.3",
    "@sapphire/time-utilities": "^1.4.6",
    "@sapphire/type": "^2.1.1",
    "@sapphire/utilities": "^3.0.6",
    "@zikeji/hypixel": "github:muchnameless/node-hypixel",
    "bufferutil": "^4.0.5",
    "chartjs-node-canvas": "^4.1.4",
    "common-tags": "^1.8.0",
    "cron": "^1.8.2",
    "discord.js": "github:discordjs/discord.js#204aee799acf08ac5b5c782a7a602c25db19354d",
    "dotenv": "^10.0.0",
    "emoji-regex": "^10.0.0",
    "form-data": "^4.0.0",
    "jaro-winkler": "^0.2.8",
    "keyv": "^4.0.4",
    "lex": "^1.7.9",
    "lilyweight": "^1.3.1",
    "minecraft-data": "^2.95.0",
    "minecraft-protocol": "^1.26.5",
    "ms": "^2.1.3",
    "node-fetch": "^3.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino": "^7.0.5",
    "prismarine-chat": "^1.3.3",
    "readdirp": "^3.6.0",
    "sequelize": "^6.9.0",
    "sinful-math": "^0.0.1",
    "timezonecomplete": "^5.12.4",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.7"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cron": "^1.7.3",
    "@types/keyv": "^3.1.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/validator": "^13.6.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "discord-api-types": "^0.24.0",
    "eslint": "^8.1.0",
    "eslint-plugin-regexp": "^1.5.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "pino-pretty": "^7.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.0-dev.20211101"
  },
  "engines": {
    "node": ">=v17.0.1",
    "npm": ">=8.1.2"
  }
}
