		"@sapphire/async-queue": "1.3.2-next.688ed4a.0",
		"@sapphire/shapeshift": "3.5.1-next.31b762e.0",
		"@sapphire/utilities": "3.7.1-next.688ed4a.0",
		"discord-api-types": "0.36.2-next.f2c3451.1657734776",
		"discord.js@14.0.0-dev.1657411900-f0b68d5": "patch:discord.js@npm:14.0.0-dev.1657411900-f0b68d5#.yarn/patches/discord.js-npm-14.0.0-dev.1657411900-f0b68d5-c14126668c.patch",
		"minecraft-protocol@^1.35.0": "patch:minecraft-protocol@npm:1.35.0#.yarn/patches/minecraft-protocol-npm-1.35.0-d5ba667cea.patch"
	}
