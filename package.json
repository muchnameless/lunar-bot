{
  "name": "lunar-bot-dev",
  "version": "1.0.0",
  "type": "module",
  "description": "dev version of the lunar guard discord bot",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "update": "git pull && npm run build && npm run start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --no-stack-trace --verbose --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "start": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
    "deploy:commands": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/deploy.js",
    "maintenance": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/maintenance.js",
    "db": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/structures/database/dbInit.js --alter"
  },
  "author": "muchnameless",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@keyv/redis": "^2.1.2",
    "@sapphire/async-queue": "^1.1.5",
    "@sapphire/stopwatch": "^1.1.4",
    "@sapphire/type": "^2.1.0",
    "@zikeji/hypixel": "^3.1.1",
    "bufferutil": "^4.0.4",
    "chalk": "^4.1.2",
    "chartjs-node-canvas": "^3.2.0",
    "common-tags": "^1.8.0",
    "cron": "^1.8.2",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "emoji-regex": "^10.0.0",
    "form-data": "^4.0.0",
    "jaro-winkler": "^0.2.8",
    "keyv": "^4.0.3",
    "lex": "^1.7.9",
    "lilyweight": "^1.3.1",
    "minecraft-data": "^2.94.0",
    "minecraft-protocol": "^1.26.5",
    "ms": "^2.1.3",
    "node-fetch": "^3.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "prismarine-chat": "^1.3.3",
    "readdirp": "^3.6.0",
    "rss-parser": "^3.12.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sinful-math": "^0.0.1",
    "timezonecomplete": "^5.12.4",
    "tslib": "^2.3.1",
    "undici": "^4.7.0",
    "utf-8-validate": "^5.0.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cron": "^1.7.3",
    "@types/keyv": "^3.1.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.10.2",
    "@types/pg": "^8.6.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "discord-api-types": "^0.23.1",
    "eslint": "^7.32.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.0-dev.20211005"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7.24.2"
  }
}
