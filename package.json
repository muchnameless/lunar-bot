		"ley": "^0.8.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^9.1.1",
		"pretty-quick": "^3.1.3",
		"ts-prune": "^0.10.3",
		"typescript": "^4.9.0-dev.20221022"
	},
	"engines": {
		"node": ">=v19.0.0"
	},
	"volta": {
		"node": "19.0.0"
	},
	"packageManager": "yarn@3.2.4",
	"resolutions": {
		"@discordjs/builders": "1.3.1-dev.1666397710-b2658ce.0",
		"@discordjs/collection": "1.2.1-dev.1666397721-b2658ce.0",
