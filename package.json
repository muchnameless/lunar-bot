		"@sapphire/async-queue": "patch:@sapphire/async-queue@npm:1.5.1-next.1562630.0#.yarn/patches/@sapphire-async-queue-npm-1.5.1-next.6b2c3aa.0-b9d98a9df6.patch",
		"@sapphire/shapeshift": "3.5.2-next.bb91ddd.0",
		"@sapphire/snowflake": "3.2.3-next.1562630.0",
		"@types/node": "patch:@types/node@npm:^18.7.13#.yarn/patches/@types-node-npm-18.7.2-8575dab8c2.patch",
		"discord-api-types": "0.37.5-next.98889.1661293409",
		"discord.js": "patch:discord.js@14.3.1-dev.1661688276-682e0e1.0#./.yarn/patches/discord.js-npm-14.3.1-dev.1661429072-13baf75.0-a61789f7e5.patch",
		"minecraft-protocol": "patch:minecraft-protocol@npm:1.36.1#.yarn/patches/minecraft-protocol-npm-1.36.0-42ba3c97ef.patch"
	}
