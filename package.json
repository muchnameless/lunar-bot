		"@sapphire/utilities": "3.9.1-next.b51fee4.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#b8e9632211c09bb6e97c7a9a96fe34109fcb2055",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.9.1",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.1.0",
