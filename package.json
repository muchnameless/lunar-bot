		"@sapphire/shapeshift": "3.5.2-next.edd92b9.0",
		"@sapphire/snowflake": "3.2.3-next.b51fee4.0",
		"discord-api-types": "0.37.2-next.4732f1a.1660142287",
		"discord.js@14.2.1-dev.1660176731-7666a6c.0": "patch:discord.js@npm:14.2.1-dev.1660176731-7666a6c.0#.yarn/patches/discord.js-npm-14.2.1-dev.1660176731-7666a6c.0-9097cb77b2.patch",
		"minecraft-protocol@1.35.1": "patch:minecraft-protocol@npm:1.35.1#.yarn/patches/minecraft-protocol-npm-1.35.1-bc03e71596.patch"
	}
