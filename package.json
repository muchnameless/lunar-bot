		"discord-api-types": "0.34.0-next.f49546c.1655104606",
		"discord.js@14.0.0-dev.1655165434-b4e28a8": "patch:discord.js@npm:14.0.0-dev.1655165434-b4e28a8#.yarn/patches/discord.js-npm-14.0.0-dev.1655165434-b4e28a8-a66e9453ff.patch",
		"sequelize@6.20.1": "patch:sequelize@npm:6.20.1#.yarn/patches/sequelize-npm-6.20.1-38ae7d1966.patch"
	}
}
