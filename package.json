		"@types/cron": "^2.0.0",
		"@types/node": "^18.7.18",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.7.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.23.1",
		"eslint-config-neon": "^0.1.33",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^13.0.3",
		"pino-pretty": "^9.1.0",
		"pretty-quick": "^3.1.3",
		"ts-prune": "^0.10.3",
		"typescript": "^4.9.0-dev.20220918"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.3",
	"resolutions": {
		"@discordjs/builders": "patch:@discordjs/builders@1.2.1-dev.1663546265-6d43e26.0#./.yarn/patches/@discordjs-builders-npm-1.2.1-dev.1662379463-4ffdada.0-c1cf53aeb3.patch",
		"@discordjs/collection": "1.1.1-dev.1663546266-6d43e26.0",
