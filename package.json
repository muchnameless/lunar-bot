		"@sapphire/utilities": "3.9.4-next.7392d04.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#38880e95836b3cbfc2b012bb4960c1a19b32acb7",
		"bufferutil": "^4.0.6",
