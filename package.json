		"@sapphire/utilities": "^3.7.0",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#b8e9632211c09bb6e97c7a9a96fe34109fcb2055",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.8.0",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.0.0",
		"discord.js": "14.0.0-dev.1657411900-f0b68d5",
		"fast-xml-parser": "^4.0.8",
		"ioredis": "^5.1.0",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
