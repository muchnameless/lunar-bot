{
  "name": "lunar-bot",
  "version": "2.0.0",
  "type": "module",
  "description": "dev version of the lunar guard discord bot",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "npm run clean && tsc --watch",
    "update": "git pull && npm run build && npm run start",
    "test": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/*",
    "prepare": "husky install",
    "start": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
    "start:dev": "npm start | pino-pretty",
    "dev": "npm run build && npm run start:dev",
    "deploy:commands": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/deploy.js",
    "maintenance": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/maintenance.js",
    "db": "node --require dotenv/config --enable-source-maps --es-module-specifier-resolution=node dist/scripts/dbInit.js --alter | pino-pretty"
  },
  "author": "muchnameless",
  "license": "GPL-3.0",
  "dependencies": {
    "@discordjs/builders": "github:muchnameless/builders#preview",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@keyv/redis": "^2.2.1",
    "@sapphire/async-queue": "^1.1.9",
    "@sapphire/stopwatch": "^1.2.4",
    "@sapphire/time-utilities": "^1.5.0",
    "@sapphire/type": "^2.1.2",
    "@sapphire/utilities": "^3.1.0",
    "@zikeji/hypixel": "github:muchnameless/node-hypixel",
    "bufferutil": "^4.0.5",
    "chartjs-node-canvas": "^4.1.5",
    "common-tags": "^1.8.2",
    "cron": "^1.8.2",
    "discord.js": "github:discordjs/discord.js#6e57b65e99389eb8cbc8c2112281a9c1b7d3783a",
    "dotenv": "^10.0.0",
    "emoji-regex": "^10.0.0",
    "formdata-polyfill": "^4.0.10",
    "jaro-winkler": "^0.2.8",
    "js-big-decimal": "^1.3.4",
    "keyv": "^4.0.4",
    "lex": "^1.7.9",
    "lilyweight": "^2.0.0",
    "minecraft-data": "^2.97.0",
    "minecraft-protocol": "^1.28.1",
    "ms": "^2.1.3",
    "node-fetch": "^3.1.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino": "^7.5.1",
    "prismarine-chat": "^1.4.0",
    "re2": "^1.17.1",
    "readdirp": "^3.6.0",
    "sequelize": "^6.11.0",
    "timezonecomplete": "^5.12.4",
    "tiny-typed-emitter": "^2.1.0",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.7"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cron": "^1.7.3",
    "@types/keyv": "^3.1.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.1",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "discord-api-types": "^0.25.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.0-dev.20211209"
  },
  "engines": {
    "node": ">=v17.0.1",
    "npm": ">=8.2.0"
  }
}
