{
	"name": "lunar-bot",
	"version": "2.0.0",
	"type": "module",
	"description": "dev version of the lunar guard discord bot",
	"main": "dist/index.js",
	"scripts": {
		"clean": "node scripts/clean.mjs",
		"build": "yarn clean && tsc",
		"update": "yarn upgrade-interactive",
		"test": "tsc --noEmit",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write src/*",
		"prepare": "husky install",
		"start": "env-cmd node dist/index.js | tee -a logs/lunar-bot-out.log",
		"start:dev": "NODE_ENV=development yarn start | pino-pretty",
		"dev": "yarn build; yarn start:dev",
		"deploy:commands": "env-cmd node dist/scripts/deploy.js | pino-pretty",
		"db": "env-cmd node dist/scripts/dbInit.js --alter | pino-pretty",
		"migrate": "env-cmd ley"
	},
	"author": "muchnameless",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/builders": "*",
		"@discordjs/collection": "*",
		"@discordjs/rest": "*",
		"@sapphire/async-queue": "^1.3.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#6da21ece522ea8dd61f44b5c74ab64b2f0e8be82",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.7.1",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.0.0",
		"discord-api-types": "*",
		"discord.js": "14.0.0-dev.1651493371-4ba0f56",
		"emoji-regex": "9.2.2",
		"fast-xml-parser": "^4.0.7",
		"ioredis": "^5.0.4",
		"js-big-decimal": "^1.3.4",
		"lex": "^1.7.9",
		"lilyweight": "^2.6.2",
		"minecraft-data": "^3.1.1",
		"minecraft-protocol": "^1.34.0",
		"ms": "^2.1.3",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"pino": "^7.11.0",
		"postgres": "3.1.0",
		"prettier": "^2.6.2",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.4",
		"sequelize": "6.19.0",
		"timezonecomplete": "^5.12.4",
		"tiny-typed-emitter": "^2.1.0",
		"tslib": "^2.4.0",
		"undici": "^5.2.0",
		"utf-8-validate": "^5.0.9"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.4",
		"@commitlint/config-angular": "^16.2.4",
		"@types/common-tags": "^1.8.1",
		"@types/cron": "^2.0.0",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.31",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.6.0",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-regexp": "^1.7.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^12.4.1",
		"pino-pretty": "^7.6.1",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.8.0-dev.20220511"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.0",
	"resolutions": {
		"@discordjs/builders": "0.14.0-dev.1651493044-4ba0f56",
		"@discordjs/collection": "0.7.0-dev.1651493036-4ba0f56",
		"@discordjs/rest": "0.5.0-dev.1652350338-4515a1e",
		"discord-api-types": "0.32.2-next.8f16f45.1652220324",
		"discord.js@14.0.0-dev.1651493371-4ba0f56": "patch:discord.js@npm:14.0.0-dev.1651493371-4ba0f56#.yarn/patches/discord.js-npm-14.0.0-dev.1651493371-4ba0f56-66893dd9e2.patch"
	}
}
