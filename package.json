{
	"name": "lunar-bot",
	"version": "2.0.0",
	"type": "module",
	"description": "dev version of the lunar guard discord bot",
	"main": "dist/index.js",
	"imports": {
		"#api": "./dist/api/index.js",
		"#chatBridge/*": "./dist/lib/chatBridge/*",
		"#chatBridge/constants": "./dist/lib/chatBridge/constants/index.js",
		"#constants": "./dist/lib/constants/index.js",
		"#db": "./dist/lib/structures/database/index.js",
		"#functions": "./dist/lib/functions/index.js",
		"#logger": "./dist/logger.js",
		"#networth/*": "./dist/lib/networth/*",
		"#structures/*": "./dist/lib/structures/*",
		"#types": "./dist/lib/types/util.js",
		"#utils": "./dist/lib/util/index.js",
		"#root/*": "./dist/*"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "yarn clean && tsc -b src",
		"update": "yarn upgrade-interactive",
		"test": "tsc -b src",
		"lint": "eslint src --ext ts --cache",
		"format": "prettier --cache --write . && eslint src --ext ts --fix --cache",
		"prepare": "husky install",
		"start": "scripts/save_commit.sh && env-cmd node . | tee -a logs/lunar-bot-out.log",
		"start:dev": "NODE_ENV=development yarn start | pino-pretty",
		"dev": "yarn build; yarn start:dev",
		"deploy:commands": "env-cmd node dist/scripts/deploy.js | pino-pretty",
		"db": "env-cmd node dist/scripts/dbInit.js --alter | pino-pretty",
		"migrate": "env-cmd ley"
	},
	"author": "muchnameless",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/builders": "1.3.0-dev.1662293019-8b3d006.0",
		"@discordjs/collection": "1.2.0-dev.1662293043-8b3d006.0",
