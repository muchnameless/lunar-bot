		"pino": "^8.7.0",
		"postgres": "3.3.1",
		"prettier": "^2.7.1",
		"prismarine-chat": "^1.7.2",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "^6.25.3",
		"tslib": "^2.4.0",
		"undici": "^5.11.0",
		"utf-8-validate": "^5.0.10",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-angular": "^17.1.0",
		"@types/common-tags": "^1.8.1",
		"@types/cron": "^2.0.0",
		"@types/luxon": "^3",
		"@types/node": "^18.11.3",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.7.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.26.0",
