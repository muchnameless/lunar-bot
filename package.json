		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#3161e8e687d869b17a1eb25a869ec75f0adab3a7",
		"bufferutil": "^4.0.7",
		"chart.js": "^3.9.1",
		"common-tags": "^1.8.2",
		"cron": "^2.1.0",
		"discord.js": "14.6.1-dev.1666397720-b2658ce.0",
		"fast-xml-parser": "^4.0.11",
		"ioredis": "^5.2.3",
		"js-big-decimal": "^1.3.10",
		"lilyweight": "^2.7.0",
