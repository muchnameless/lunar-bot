{
	"name": "lunar-bot",
	"version": "2.0.0",
	"type": "module",
	"description": "dev version of the lunar guard discord bot",
	"main": "dist/index.js",
	"scripts": {
		"clean": "node scripts/clean.mjs",
		"build": "yarn clean && tsc",
		"update": "yarn upgrade-interactive",
		"test": "tsc --noEmit",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write src/*",
		"prepare": "husky install",
		"start": "env-cmd node dist/index.js | tee -a logs/lunar-bot-out.log",
		"start:dev": "NODE_ENV=development yarn start | pino-pretty",
		"dev": "yarn build; yarn start:dev",
		"deploy:commands": "env-cmd node dist/scripts/deploy.js | pino-pretty",
		"db": "env-cmd node dist/scripts/dbInit.js --alter | pino-pretty",
		"migrate": "env-cmd ley"
	},
	"author": "muchnameless",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/rest": "*",
		"@sapphire/async-queue": "^1.3.1",
		"@sapphire/discord-utilities": "^2.11.2",
		"@sapphire/shapeshift": "^3.2.0",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/jaro-winkler": "^1.1.0",
		"@zikeji/hypixel": "github:muchnameless/node-hypixel#5d0af5aedf8d9f6748d9e03a183a8dde3bc74338",
		"bufferutil": "^4.0.6",
		"chart.js": "^3.8.0",
		"chartjs-node-canvas": "^4.1.6",
		"common-tags": "^1.8.2",
		"cron": "^2.0.0",
		"discord.js": "14.0.0-dev.1655165434-b4e28a8",
		"fast-xml-parser": "^4.0.8",
		"ioredis": "^5.0.6",
		"js-big-decimal": "github:muchnameless/js-big-decimal",
		"lex": "^1.7.9",
		"lilyweight": "^2.6.2",
		"minecraft-data": "^3.4.0",
		"minecraft-protocol": "^1.35.0",
		"ms": "^2.1.3",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"pino": "^8.0.0",
		"postgres": "3.2.4",
		"prettier": "^2.7.0",
		"prismarine-chat": "^1.6.1",
		"prismarine-nbt": "^2.2.1",
		"re2": "^1.17.7",
		"sequelize": "6.20.1",
		"timezonecomplete": "^5.12.4",
		"tslib": "^2.4.0",
		"undici": "^5.5.1",
		"utf-8-validate": "^5.0.9"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-angular": "^17.0.0",
		"@types/common-tags": "^1.8.1",
		"@types/cron": "^2.0.0",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.42",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.6.3",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-regexp": "^1.7.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"husky": "^8.0.1",
		"ley": "^0.7.1",
		"lint-staged": "^13.0.1",
		"pino-pretty": "^8.0.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^4.8.0-dev.20220615"
	},
	"engines": {
		"node": ">=v18.0.0",
		"npm": ">=8.7.0"
	},
	"packageManager": "yarn@3.2.1",
	"resolutions": {
		"@discordjs/builders": "0.16.0-dev.1655165432-b4e28a8",
		"@discordjs/builders@^0.16.0-dev": "patch:@discordjs/builders@npm:0.16.0-dev.1654949068-96053ba#.yarn/patches/@discordjs-builders-npm-0.16.0-dev.1654949068-96053ba-5aff0216b3.patch",
		"@discordjs/collection:": "0.8.0-dev.1655165444-b4e28a8",
